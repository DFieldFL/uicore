{
  "eslint.enable": true,
  "search.exclude": {
    "**/node_modules": true,
    "npm-debug.log*": true,
    "**/static": true,
    "dist/": true,
    "yarn-error.*": true,
    "**/.next/": true,
    "**/next-env.d.ts": true,
    "**/next.config.js": true,
    "docs/package.json": true,
    "docs/tsconfig.json": true,
    "**/yarn.lock": true,
    "**/.mdx-data": true,
    "**/_app.js": true,
    "webpack.config.js": true,
    "tsconfig.json": true,
    "cypress.json": true,
    "types": true,
    "scripts": true,
    ".*": true,
    "*.config.js": true
  },
  "files.exclude": {
    "**/node_modules": false,
    "npm-debug.log*": false,
    "dist/": false,
    "yarn-error.*": false,
    "**/yarn.lock": false
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
