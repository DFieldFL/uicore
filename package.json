{
  "name": "ui-platform",
  "version": "0.0.1",
  "description": "UI Platform",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "style": "dist/index.css",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "start": "run-p -c ui:tsd ui:watch docs:watch",
    "build": "NODE_ENV=production webpack --mode=production",
    "clean": "rm -rf docs/static/index.* dist/*",
    "ui:tsd": "tsc --emitDeclarationOnly --watch --declarationDir ./docs/static/",
    "ui:icons": "node ./scripts/build-icons.js > ./src/icons/index.ts",
    "ui:watch": "NODE_ENV=development webpack --mode=development --watch",
    "docs:watch": "cd docs; yarn watch",
    "docs:build": "cd docs; yarn build",
    "docs:deploy": "cd docs; echo To be defined"
  },
  "private": true,
  "peerDependencies": {
    "@blueprintjs/core": "^3.18.0",
    "clipboard-copy": "^3.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.18.0",
    "@svgr/webpack": "^4.3.2",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "clipboard-copy": "^3.1.0",
    "css-loader": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "glob": "^7.1.4",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.2",
    "postcss-nested": "^4.1.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-node-externals": "^1.7.2"
  }
}
